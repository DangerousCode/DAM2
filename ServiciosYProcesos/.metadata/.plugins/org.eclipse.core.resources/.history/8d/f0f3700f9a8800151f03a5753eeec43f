package creador;

public class Paquete {
	char direccion;
	int peso;
	Paquete(){
		
	}
	Paquete(char direccion,int peso){
		this.direccion=direccion;
		this.peso=peso;
	}
	public char getDireccion() {
		return direccion;
	}
	public void setDireccion(char direccion) {
		this.direccion = direccion;
	}
	public float getPeso() {
		return peso;
	}
	public void setPeso(int peso) {
		this.peso = peso;
	}
}
