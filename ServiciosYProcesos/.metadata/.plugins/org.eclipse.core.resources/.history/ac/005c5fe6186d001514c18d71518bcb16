package Main;

public class Main extends Thread{
	public static void main(String[] args){
		Hilo hilo1=new Hilo(1);
		Hilo hilo2=new Hilo(2);
		Hilo hilo3=new Hilo(3);
		Hilo hilo4=new Hilo(4);
		
		hilo1.run();
		hilo2.run();
		hilo3.run();
		hilo4.run();
		
		System.out.println("La suma de numeros naturales es: "+hilo1.naturales);
		System.out.println("La multiplicacion de numeros naturales es: "+hilo2.multiplica);
		System.out.println("La suma de pares es: "+hilo3.pares);
		System.out.println("La suma de impares es: "+hilo4.impares);
		
	}
}
